    ".github/workflows/", // prevent removing old tests to trigger everything
    ".gitignore",
    ".kokoro/",
    ".prettierignore",
    ".prettierrc.js",
    "cloud-samples-tools", // checked out by GH action in ci-*.yml
    "CODEOWNERS",
