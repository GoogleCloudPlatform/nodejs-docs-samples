{
  "name": "cloudsql-postgres",
  "description": "Node.js PostgreSQL sample for Cloud SQL.",
  "version": "0.0.1",
  "private": true,
  "license": "Apache-2.0",
  "author": "Google Inc.",
  "repository": {
    "type": "git",
    "url": "https://github.com/GoogleCloudPlatform/nodejs-docs-samples.git"
  },
  "engines": {
    "node": "20.8.0"
  },
  "scripts": {
    "start": "node server/server.js",
    "start-proxy": "! pgrep cloud_sql_proxy > /dev/null && cloud_sql_proxy -dir=/cloudsql -instances=$INSTANCE_CONNECTION_NAME &",
    "test": "c8 mocha -p -j 2 test/*.test.js --timeout=60000 --exit"
  },
  "dependencies": {
    "@google-cloud/cloud-sql-connector": "0.5.1",
    "@google-cloud/functions-framework": "3.3.0",
    "@google-cloud/logging-winston": "6.0.0",
    "@google-cloud/secret-manager": "5.0.1",
    "express": "4.18.2",
    "knex": "2.5.1",
    "pg": "8.11.3",
    "pug": "3.0.2",
    "winston": "3.11.0"
  },
  "devDependencies": {
    "c8": "8.0.1",
    "mocha": "10.2.0",
    "supertest": "6.3.3"
  }
}
