{
  "name": "appengine-endpoints",
  "description": "Endpoints Node.js gRPC sample for Google App Engine",
  "version": "0.0.1",
  "private": true,
  "license": "Apache-2.0",
  "author": "Google Inc.",
  "repository": {
    "type": "git",
    "url": "https://github.com/GoogleCloudPlatform/nodejs-docs-samples.git"
  },
  "engines": {
    "node": ">=4.3.2"
  },
  "scripts": {
    "start": "node server.js",
    "system-test": "ava -T 30s --verbose system-test/*.test.js",
    "test": "npm run system-test"
  },
  "dependencies": {
    "body-parser": "1.17.2",
    "express": "4.15.3",
    "grpc": "1.3.8",
    "yargs": "8.0.2"
  },
  "devDependencies": {
    "@google-cloud/nodejs-repo-tools": "1.4.15",
    "ava": "0.19.1"
  },
  "cloud-repo-tools": {
    "test": {
      "app": {
        "requiredEnvVars": [
          "ENDPOINTS_API_KEY",
          "ENDPOINTS_GCE_HOST",
          "ENDPOINTS_GKE_HOST"
        ]
      }
    }
  }
}
