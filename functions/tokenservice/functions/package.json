    "@google-cloud/nodejs-repo-tools": "^3.3.0",
    "eslint": "^6.0.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.0.1",
    "mocha": "^6.0.2",
    "prettier": "1.18.2",
    "supertest": "^4.0.0"
  },
  "engines": {
    "node": "8"
  },
  "private": true
