{
  "name": "nodejs-docs-samples-iot-manager",
  "version": "0.0.1",
  "description": "Example of Cloud IoT device administration",
  "main": "manager.js",
  "license": "Apache-2.0",
  "author": "Google Inc.",
  "repository": {
    "type": "git",
